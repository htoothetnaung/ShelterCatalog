{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "dist/petfinder-cats/browser/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/dist/petfinder-cats/browser/assets/$1"
    },
    {
      "src": "/(.*).(js|css|ico|json|png|jpg|jpeg|svg)",
      "dest": "/dist/petfinder-cats/browser/$1.$2"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ]
}