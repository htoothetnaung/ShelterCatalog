{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist/petfinder-catanddog",
  "routes": [
    { "src": "/browser/(.*)", "dest": "/browser/$1" },
    { "handle": "filesystem" },
    { "src": "/assets/(.*)", "dest": "/browser/assets/$1" },
    { "src": "/(.*).(js|css|ico|jpg|jpeg|png|gif|svg)", "dest": "/browser/$1.$2" },
    { "src": "/(.*)", "dest": "/browser/index.html" }
  ]
}